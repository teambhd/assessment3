<?xml version="1.0" encoding="UTF-8"?>

<!-- TO GET THE PROJECT RUNNING IN ECLIPSE: -->
<!-- 1. Change all the references to macosx to match your platform (either windows, macosx or linux). -->
<!-- 2. Symlink or copy this file to .classpath in the same directory. -->

<classpath>
	<classpathentry kind="src" path="src"/>
	<classpathentry kind="lib" path="lib/jars/lwjgl.jar">
		<attributes>
			<attribute name="org.eclipse.jdt.launching.CLASSPATH_ATTR_LIBRARY_PATH_ENTRY" value="Game/lib/natives/macosx"/>
		</attributes>
	</classpathentry>
	<classpathentry kind="lib" path="lib/jars/slick.jar" sourcepath="lib/jars/lwjgl.jar">
		<attributes>
			<attribute name="org.eclipse.jdt.launching.CLASSPATH_ATTR_LIBRARY_PATH_ENTRY" value="Game/lib/natives/macosx"/>
		</attributes>
	</classpathentry>
	<classpathentry kind="lib" path="lib/jars/jogg-0.0.7.jar"/>
	<classpathentry kind="lib" path="lib/jars/jorbis-0.0.17-1.jar"/>
	<classpathentry kind="con" path="org.eclipse.jdt.junit.JUNIT_CONTAINER/4"/>
	<classpathentry kind="con" path="org.eclipse.jdt.launching.JRE_CONTAINER"/>
	<classpathentry kind="output" path="bin"/>
</classpath>
